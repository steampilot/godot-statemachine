; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot-StateMachine"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Audio="*res://Globals/AUDIO.gd"
MusicPlayer="*res://Globals/MUSIC_PLAYER.tscn"
LevelLoader="*res://Globals/LEVEL_LOADER.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[gui]

theme/custom_font="res://Fonts/PixelOperator8.ttf"
theme/default_theme="res://Resources/Themes/default_theme.tres"

[rendering]

textures/canvas_textures/default_texture_filter=0
2d/snap/snap_2d_transforms_to_pixel=true
